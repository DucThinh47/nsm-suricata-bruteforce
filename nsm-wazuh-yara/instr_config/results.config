wazuh-manager = monitor:.bash_history : CONTAINS : sudo bash ./wazuh-install.sh -a -p 8080
wazuh-agent = victim:/var/ossec/etc/ossec.conf : CONTAINS : 192.168.11.10
yara-check = victim:yara.stdout : CONTAINS : 4.5
_yara_rules = victim:ls.stdout : CONTAINS : yara_rules.yar
_yara_shell = victim:ls.stdout : CONTAINS : yara.sh
yara-agent = victim:/var/ossec/etc/ossec.conf : CONTAINS : <directories realtime="yes">/tmp/</directories>
_local_rules = monitor:/var/ossec/etc/rules/local_rules.xml : CONTAINS : File "$(yara_scanned_file)" is a positive match. Yara rule: $(yara_rule)
_local_decoder = monitor:/var/ossec/etc/decoders/local_decoder.xml : CONTAINS : wazuh-yara: (\S+) - Scan result: (\S+) (\S+)
_manager_conf = monitor:/var/ossec/etc/ossec.conf : CONTAINS : yara_linux
malware-attack = attacker:curl.stdout : CONTAINS : <form method="GET" name="ping">
wazuh-log = monitor:yara-answer.txt : CONTAINS : MAL_ELF_LNX_Mirai_Oct10_2_RID2F3A
_hash = monitor:/home/ubuntu/yara-answer.txt : SEARCH : MSV:{:S} : HAVESTRING : MSV
