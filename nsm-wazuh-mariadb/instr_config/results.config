#DOC: Cai dat thanh cong Wazuh Manager
wazuh-manager = monitor:.bash_history : CONTAINS : sudo bash ./wazuh-install.sh -a -p 8080
#DOC: Cai dat thanh cong Wazuh Agent va ket noi toi Manager
wazuh-agent = server:/var/ossec/etc/ossec.conf : CONTAINS : 192.168.25.10
_mariadb_install1 = server:.bash_history : CONTAINS : sudo apt install mariadb-server mariadb-client -y
_mariadb_install2 = server:.bash_history : CONTAINS : sudo chown -R mysql:mysql /var/log/mysql
_mariadb_install3 = server:.bash_history : CONTAINS : sudo chmod -R 755 /var/log/mysql
#DOC: Cau hinh co che xac thuc MariaDB

root-conf = server:.bash_history : CONTAINS : sudo mysql_secure_installation
_mariadb_conf1 = server:/etc/mysql/my.cnf : CONTAINS : [server]
_mariadb_conf2 = server:/etc/mysql/my.cnf : CONTAINS : [mariadb]
_mariadb_conf3 = server:/etc/mysql/my.cnf : CONTAINS : [mysqld]
_mariadb_decoder = monitor:/var/ossec/etc/decoders/mariadb_decoders.xml : CONTAINS : mariadatabase-decoder
_mariadb_rules = monitor:/var/ossec/etc/rules/mariadb_rules.xml : CONTAINS : Successful operation
_wazuh_log1 = monitor:/var/ossec/logs/alerts/alerts.log : CONTAINS : 100701
_wazuh_log2 = monitor:/var/ossec/logs/alerts/alerts.log : CONTAINS : 100702
